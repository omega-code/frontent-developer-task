{
  "compilerOptions": {
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6",
    "jsx": "react",
    "module": "commonjs",
    "outDir": "./build/"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "include": [
    "./src/**/*"
  ]
}
